    "test:screenshot:approve": "loki approve",
    "storybook": "start-storybook -c ./config/storybook -p 6006",
    "storybook:build": "build-storybook -c ./config/storybook",
    "visual:report:json": "node scripts/generate-visual-json-report.js",
    "visual:report:html": "reg-cli --from .loki/report.json --report .loki/report.html"
